<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <TextField fx:id="usernameField" alignment="CENTER" layoutX="137.0" layoutY="176.0" promptText="Username" />
      <TextField fx:id="passwordField" alignment="CENTER" layoutX="137.0" layoutY="229.0" promptText="Password" />
      <Button layoutX="190.0" layoutY="331.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
      <Label fx:id="instructions" alignment="CENTER" contentDisplay="CENTER" layoutX="35.0" layoutY="94.0" prefHeight="41.0" prefWidth="357.0" text="Please enter your username and password:" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label fx:id="title" alignment="CENTER" contentDisplay="CENTER" layoutX="43.0" layoutY="39.0" prefHeight="30.0" prefWidth="340.0" text="Scheduling Application" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Label>
      <Label fx:id="userLocation" alignment="CENTER" contentDisplay="CENTER" layoutX="126.0" layoutY="274.0" prefHeight="17.0" prefWidth="170.0" text="User location:" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label fx:id="location" alignment="CENTER" contentDisplay="CENTER" layoutX="43.0" layoutY="301.0" prefHeight="20.0" prefWidth="340.0" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font></Label>
   </children>
</AnchorPane>
